SELECT TOP (1000) [DBGruppo]
      ,[Esercizio]
      ,[CodArt]
      ,[VarianteArt]
      ,[ImpEntrateVal]
      ,[QtaEntrateVal]
      ,[ImpGiacInizialeVal]
      ,[QtaGiacInizialeVal]
