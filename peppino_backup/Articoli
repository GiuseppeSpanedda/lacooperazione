  SELECT TOP (1000) [DBGruppo]
      ,[CodArt]
      ,[FirmaUltVarData]
      ,[QtaGiacUmMag]
      ,[QtaGiacUm2Mag]
  FROM [ESOLV].[dbo].[EMPGFQART]

  SELECT TOP (1000) [DBGruppo]
      ,[CodArt]
      ,[CodMag]
      ,[CodAreaMag]
      ,[FirmaUltVarData]
      ,[QtaGiacUmMag]
      ,[QtaGiacUm2Mag]
  FROM [ESOLV].[dbo].[EMPGFQARTMAGAREA]

  SELECT TOP (1000) [DBGruppo]
      ,[CodArt]
      ,[VarianteArt]
      ,[FirmaUltVarData]
      ,[QtaGiacUmMag]
      ,[QtaGiacUm2Mag]
  FROM [ESOLV].[dbo].[EMPGFQARTVAR]

SELECT TOP (1000) [DBGruppo]
      ,[CodArt]
      ,[VarianteArt]
      ,[CodMag]
      ,[FirmaUltVarData]
      ,[QtaGiacUmMag]
      ,[QtaGiacUm2Mag]
