  SELECT TOP (1000) [DBGruppo]
      ,[CodArt]
      ,[VarianteArt]
      ,[CodMag]
      ,[QtaGiacUm2Mag]
      ,[QtaGiacUmMag]
      ,[CodArea]
  FROM [ESOLV].[dbo].[ESV_GIAC_AVMA_COLL]

  SELECT TOP (1000) * FROM [ESOLV].[dbo].[ESV_GIAC_MA];
  SELECT TOP (1000) * FROM [ESOLV].[dbo].[ESV_GIAC_LMA];
