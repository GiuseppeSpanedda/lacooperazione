########################### Articoli ####################################

IMPORTANTE:recuperare la marca dal codice articolo, le iniziali equivalgono a una marca specifica

	SELECT TOP (1000) A.CodArt, RicercaAlternativa, DesArt, TipoArt, ClasseMerceologica, CodIva,MagUm, VenPercRicarico, CodIvaAcquisti, CodBarre, Des
  FROM [ESOLV].[dbo].[ArtCodiciBarre] B JOIN [ESOLV].[dbo].[ArtAnagrafica] A ON ( A.CodArt=B.CodArt )
  ;

  /*------------------------
USE peppino

SELECT 
      A.CodArt AS 'articolo.id_art', 
      RicercaAlternativa AS 'articolo.ric_alt', 
      DesArt AS 'articolo.descrizione', 
      ClasseMerceologica AS 'articolo.classe'  
      FROM [ESOLV].[dbo].[ArtCodiciBarre] B JOIN [ESOLV].[dbo].[ArtAnagrafica] A ON ( A.CodArt=B.CodArt )  for json path ;

;
------------------------*/

